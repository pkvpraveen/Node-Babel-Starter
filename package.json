{
  "name": "node-babel-starter",
  "version": "1.0.0",
  "description": "Starter project to use node with es6 syntax",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon bin/dev",
    "clean": "rm -rf build",
    "build": "npm run clean && mkdir build && babel src -s -d build",
    "production": "npm run build && node bin/production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkvpraveen/node-babel-starter.git"
  },
  "author": "PKV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkvpraveen/node-babel-starter/issues"
  },
  "homepage": "https://github.com/pkvpraveen/node-babel-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.4"
  }
}
